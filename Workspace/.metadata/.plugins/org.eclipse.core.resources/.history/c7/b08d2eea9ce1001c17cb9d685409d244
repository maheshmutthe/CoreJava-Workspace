package exception05;


public class Mobile {
	public static void checkPass(String mobile) throws InvalidPassException{
		boolean result = mobile.matches("*[7-9][0-9]{10}");
		if (!result) {
			InvalidPassException e = new InvalidPassException("Invalid Mobile: "+mobile);
			throw e;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String mobile = "9988776655";
		try { 
			checkPass(mobile);
			System.out.println("Valid Mobile: "+mobile);
		}catch (InvalidPassException e) {
			System.out.println(e.getMessage());
		}
		}

}

