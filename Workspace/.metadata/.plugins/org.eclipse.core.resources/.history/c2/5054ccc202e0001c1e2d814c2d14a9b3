package Lab;

public class Assignment5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	    static int factorial(int n)
	    {
	        int j= 1, i;
	        for (i=2; i<=n; i++)
	            j *= i;
	        return j;
	    }
	      
	    public static void main(String[] args) 
	    {
	        int num = 4;
	        System.out.println("Factorial of "+ num + " is " + factorial(4));
	    }

	}

}
